
  <h2>Welcome to Your Dashboard, {{username}}!</h2>
  <h3>Your Posts:</h3>
  {{#each posts}}
    <div class="post">
      <h4>{{title}}</h4>
      <p>{{content}}</p>
      <p>Created on: {{format_date createdAt}}</p>
      <a href="/editpost/{{id}}">Edit</a> | <a href="/delete/{{id}}">Delete</a>
    </div>
  {{/each}}
  <a href="/dashboard/new">Create a New Post</a>

